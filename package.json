{
  "name": "conquadejs",
  "discrption": "A expressjs web server that caters to older technology.",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "inix": "node ./bin/www",
    "win_inix": "start conquade.bat",
    "lunix_inix": "xdg-open conquade.sh",
    "darwin_inix": "open conquade.sh",
    "start": "set DEBUG=conquade:* && set PORT=8080 && node ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  }
}
